{
  "name": "@ureq/monorepo",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.9.0",
  "scripts": {
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "clean": "turbo run clean",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "docs:dev": "pnpm --filter @ureq/docs dev",
    "docs:build": "pnpm --filter @ureq/docs build",
    "docs:preview": "pnpm --filter @ureq/docs preview",
    "playground": "pnpm --filter @ureq/playground dev",
    "playground:demo": "pnpm --filter @ureq/playground demo:all",
    "demo:basic": "turbo run demo:basic --filter=@ureq/playground",
    "demo:features": "turbo run demo:features --filter=@ureq/playground",
    "demo:interceptors": "turbo run demo:interceptors --filter=@ureq/playground",
    "demo:error-handling": "turbo run demo:error-handling --filter=@ureq/playground",
    "demo:all": "turbo run demo:all --filter=@ureq/playground"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "bumpp": "^9.3.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "tsup": "^8.3.6",
    "turbo": "^2.5.6",
    "typescript": "^5.7.3",
    "vitepress": "^1.0.0",
    "vitest": "^1.0.0"
  }
}